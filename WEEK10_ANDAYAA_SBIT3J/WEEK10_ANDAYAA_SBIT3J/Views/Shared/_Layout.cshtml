<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/fonts/flaticon.css" rel="stylesheet" />
    <link href="~/Content/animate.css" rel="stylesheet" />
    <link href="~/Content/aos.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-datepicker.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/flaticon.css" rel="stylesheet" />
    <link href="~/Content/icomoon.css" rel="stylesheet" />
    <link href="~/Content/ionicons.min.css" rel="stylesheet" />
    <link href="~/Content/jquery.timepicker.css" rel="stylesheet" />
    <link href="~/Content/magnific-popup.css" rel="stylesheet" />
    <link href="~/Content/open-iconic-bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/Content/owl.theme.default.min.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link href="~/Content/style.css" rel="stylesheet" />

</head>
<body>
    <div id="colorlib-page">
        <a href="#" class="js-colorlib-nav-toggle colorlib-nav-toggle"><i></i></a>
        <aside id="colorlib-aside" role="complementary" class="js-fullheight text-center">
            <a href="#" class="js-colorlib-nav-toggle colorlib-nav-toggle"><i></i></a>
            <h1 id="colorlib-logo">@Html.ActionLink("Maku", "Index", "Home", new { area = "" }, new { @class = "action-list colorlib-active font-color" })</h1>
            <nav id="colorlib-main-menu" role="navigation">
                <ul>
                    <li>@Html.ActionLink("Home", "Index", "Home", new { area = "" }, new { @class = "" })</li>
                    <li>@Html.ActionLink("About", "About", "Home", new { area = "" }, new { @class = "action-list  colorlib-active" })</li>
                    <li>@Html.ActionLink("Works", "Works", "Home", new { area = "" }, new { @class = "action-list  colorlib-active" })</li>
                    <li>@Html.ActionLink("Contact me", "Contact", "Home", new { area = "" }, new { @class = "action-list  colorlib-active" })</li>
                </ul>
            </nav>

            <div class="colorlib-footer">

                <p>&copy; @DateTime.Now.Year - Personal Portfolio</p>
                    <ul>
                        <li><a target="_blank" href="https://www.facebook.com/Makuuu.12/"><i class="fa-brands fa-facebook"></i></a></li>
                        <li><a target="_blank" href="https://github.com/makuu12"><i class="fa-brands fa-github"></i></a></li>
                        <li><a target="_blank" href="https://www.linkedin.com/in/mark-roel-andaya-098b81231/"><i class="fa-brands fa-linkedin-in"></i></a></li>
                    </ul>
            </div>
        </aside> <!-- END ASIDE -->
        @RenderBody()
    </div>
    

        @*
            <div>
            <footer>
                <p>&copy; @DateTime.Now.Year - Portfolio</p>
            </footer>
        </div>
        *@
    
    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->


    <script src="~/Scripts/_references.js"></script>
    <script src="~/Scripts/ai.0.22.9-build00167.js"></script>
    <script src="~/Scripts/ai.0.22.9-build00167.min.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery-1.10.2.intellisense.js"></script>
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery.validate-vsdoc.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/main.js"></script>
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="~/Scripts/respond.js"></script>
    <script src="~/Scripts/respond.min.js"></script>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
